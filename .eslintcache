mmon/axios';\nimport Loading from '../loading/Loading';\n\nconst DetailMeal = () => {\n    const [loading, setLoading] = useState(true);\n    const [detailMealApi, setDetailMealApi] = useState();\n    const [readMore, setReadMore] = useState(false)\n\n    const param = useParams();\n\n    const getDetailMealApi = async () => {\n        setLoading(true);\n        const response = await axios\n            .get(`/lookup.php?i=${param.id}`)\n            .catch((err) => console.log(\"Error:\", err));\n        if (response && response.data) {\n            console.log(\"ceva\", response.data.meals[0]);\n            setDetailMealApi(response.data.meals[0]);\n            setLoading(false);\n\n        }\n    };\n    useEffect(() => {\n        getDetailMealApi();\n    }, []);\n\n    if (loading) {\n        return (\n            <main>\n                <Loading/>\n            </main>\n        );\n    }\n\n\n    return (\n        <div className=\"container\">\n            <div className=\"header-detailMeal\">\n                <div className=\"underline\">\n                    <h2>{detailMealApi.strMeal}</h2>\n                </div>\n                <div className=\"detailMeal-page\">\n                    <div>\n                        <img src={detailMealApi.strMealThumb} className=\"photo\" alt={detailMealApi.strMeal}/>\n                    </div>\n                    <div>\n                        <h5>Instructions:</h5>\n                        <p>{readMore ? detailMealApi.strInstructions : `${detailMealApi.strInstructions.substring(0, 200)}...`}\n                            {detailMealApi.strInstructions.length > 200 &&\n                            <button className=\"showBtn\" onClick={() => setReadMore(!readMore)}>\n                                {readMore ? \"show less\" : \"read more\"}\n                            </button>}\n                        </p>\n                        <h5>Area food: {detailMealApi.strArea}</h5>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default DetailMeal;\n","/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/category/Categories.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/Carusel.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/Favorites.jsx",["120"],"import React, { useState, useEffect } from 'react';\nimport { axios, axiosSpring } from '../common/axios';\nimport FavoriteMeal from './meals/FavoriteMeal';\nimport Loading from './loading/Loading';\n\nexport default function Favorites() {\n  const [loading, setLoading] = useState(true);\n  const [mealsDb, setMealsDb] = useState([]);\n  const [mealsApi, setMealsApi] = useState([]);\n\n  const getFavoritesMealsDb = async () => {\n    const response = await axiosSpring\n      .get(`/favorites`)\n      .catch((err) => console.log('Error:', err));\n    if (response && response.data) {\n      setMealsDb(response.data);\n      return response.data;\n    }\n  };\n\n  const getMealsApi = async (meals) => {\n    let dataApi = [];\n    for (var i = 0; i < meals.length; i++) {\n      const response = await axios.get(`/lookup.php?i=${meals[i].id}`);\n      if (response && response.data) {\n        dataApi.push(response.data.meals[0]);\n      }\n    }\n    setMealsApi(dataApi);\n  };\n\n  const showMeals = async () => {\n    setLoading(true);\n    let meals = await getFavoritesMealsDb();\n    await getMealsApi(meals);\n    setLoading(false);\n  };\n\n  useEffect(() => {\n    showMeals();\n  }, []);\n\n  const handleDelete = async (id) => {\n    let removeMealDbById = mealsApi.filter((meal) => meal.idMeal !== id);\n    await axiosSpring.delete(`/favorites/${id}`);\n    setMealsApi(removeMealDbById);\n  };\n\n  if (loading) {\n    return (\n      <main>\n        <Loading />\n      </main>\n    );\n  }\n\n  return (\n    <>\n      <div className=\"table\">\n        <div>\n          <h3 className=\"text-name-category\">My favorites</h3>\n        </div>\n        <div className=\"favorites-meals\">\n          {mealsApi.map((product) => {\n            return (\n              <FavoriteMeal\n                key={product.idMeal}\n                handleDelete={handleDelete}\n                {...product}\n                price={\n                  mealsDb.filter((price) => {\n                    return price.id === parseInt(product.idMeal);\n                  })[0].price\n                }\n              />\n            );\n          })}\n        </div>\n      </div>\n    </>\n  );\n}\n","/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/Login.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/NotFound.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/ProtectedRoute.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/Logout.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/ProtectedLogin.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/Payment.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/authentication/Register.jsx",["121","122","123","124"],"import React from 'react';\nimport { useState, useRef } from 'react';\nimport { toast } from 'react-toastify';\nimport { Form, Button, Card, Alert } from 'react-bootstrap';\nimport { axiosSpring } from '../../common/axios';\nimport { Link } from 'react-router-dom';\n\nexport default function Register() {\n  const [error, setError] = useState('');\n  const firstNameRef = useRef();\n  const lastNameRef = useRef();\n  const usernameRef = useRef();\n  const emailRef = useRef();\n  const passwordRef = useRef();\n  const passwordConfirmRef = useRef();\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    register();\n\n    if (passwordRef.current.value !== passwordConfirmRef.current.value) {\n      toast.error('Passwords do not match');\n    }\n  };\n\n  const register = async () => {\n    const object = {\n      firstName: firstNameRef.current.value,\n      lastName: lastNameRef.current.value,\n      email: emailRef.current.value,\n      password: passwordRef.current.value,\n      username: usernameRef.current.value,\n    };\n\n    let res = await axiosSpring.post('/register', object);\n    if (res.status === 200 && res.data) {\n      setError('');\n      window.location.href = 'http://localhost:3000/login';\n      toast.success('Registration successful!');\n      return true;\n    }\n    toast.error('Register not successful! Please check input data');\n    return false;\n  };\n\n  return (\n    <>\n      <Card>\n        <Card.Body>\n          <h2 className=\"text-center mb-4\">Register</h2>\n          {error && <Alert variant=\"danger\">{error}</Alert>}\n          <Form onSubmit={handleSubmit}>\n            <Form.Group id=\"firstName\">\n              <Form.Label>First Name</Form.Label>\n              <Form.Control type=\"text\" ref={firstNameRef} required />\n            </Form.Group>\n            <Form.Group id=\"lastName\">\n              <Form.Label>Last Name</Form.Label>\n              <Form.Control type=\"text\" ref={lastNameRef} required />\n            </Form.Group>\n            <Form.Group id=\"username\">\n              <Form.Label>Username</Form.Label>\n              <Form.Control type=\"text\" ref={usernameRef} required />\n            </Form.Group>\n            <Form.Group id=\"email\">\n              <Form.Label>Email</Form.Label>\n              <Form.Control type=\"email\" ref={emailRef} required />\n            </Form.Group>\n            <Form.Group id=\"password\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control type=\"password\" ref={passwordRef} required />\n            </Form.Group>\n            <Form.Group id=\"password-confirm\">\n              <Form.Label>Password Confirmation</Form.Label>\n              <Form.Control type=\"password\" ref={passwordConfirmRef} required />\n            </Form.Group>\n            <Button className=\"w-100\" type=\"submit\">\n              Register\n            </Button>\n          </Form>\n        </Card.Body>\n      </Card>\n      <div className=\"w-100 text-center mt-2\">\n        Already have an account? <Link to=\"/login\">Log In</Link>\n      </div>\n    </>\n  );\n}\n","/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/common/axios.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/Title.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/LeftButton.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/loading/Loading.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/meals/Meal.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/RandomMealCarusel.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/RightButton.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/category/Category.js",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/meals/FavoriteMeal.jsx",[],"/Users/mihaicroitoru/projects/advanced/56_tt_glovo_frontend/src/components/carusel/RandomMeal.js",[],{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","severity":1,"message":"130","line":9,"column":9,"nodeType":"131","endLine":9,"endColumn":39},{"ruleId":"132","severity":1,"message":"133","line":58,"column":8,"nodeType":"134","endLine":58,"endColumn":10,"suggestions":"135"},{"ruleId":"132","severity":1,"message":"136","line":27,"column":8,"nodeType":"134","endLine":27,"endColumn":10,"suggestions":"137"},{"ruleId":"132","severity":1,"message":"138","line":42,"column":6,"nodeType":