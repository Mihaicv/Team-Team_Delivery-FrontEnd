e=\"navbar-toggler\"\r\n        type=\"button\"\r\n        data-toggle=\"collapse\"\r\n        data-target=\"#navbarNav\"\r\n        aria-controls=\"navbarNav\"\r\n        aria-expanded=\"false\"\r\n        aria-label=\"Toggle navigation\"\r\n      >\r\n        <span className=\"navbar-toggler-icon\"></span>\r\n      </button>\r\n      {/*<Link className=\"navbar-brand\">*/}\r\n      {/*  {loggedUser !== 'empty' ? Cookies.get('name') : 'not logged in'}*/}\r\n      {/*</Link>*/}\r\n    </nav>\r\n  );\r\n};\r\n\r\nexport default Navbar;\r\n","C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\authentication\\AuthContext.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\category\\Categories.js",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\meals\\DetailMeal.js",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\meals\\Meals.js",["163"],"import React, {useEffect, useState} from 'react';\r\nimport {useParams} from 'react-router-dom';\r\nimport {axios, axiosSpring} from '../../common/axios';\r\nimport Meal from './Meal';\r\nimport {toast} from 'react-toastify';\r\nimport Cookies from 'js-cookie';\r\nimport Axios from \"axios\";\r\nimport MealFilterByPrice from \"./MealFilterByPrice\"\r\n\r\nconst Meals = () => {\r\n\r\n    let token = Cookies.get('token');\r\n    const param = useParams();\r\n    const category = param.strCategory;\r\n\r\n    const [data, setData] = useState([]);\r\n    const [filterData, setFilterData] = useState([]);\r\n\r\n\r\n    const fetchData = () => {\r\n        const api = `https://www.themealdb.com/api/json/v1/1/filter.php?c=${category}`\r\n        const db = `http://localhost:8080/prices`\r\n\r\n        const getApiData = Axios.get(api)\r\n        const getPrice = Axios.get(db, {headers: {Authorization: 'Bearer ' + token}})\r\n\r\n        Axios.all([getApiData, getPrice]).then(\r\n            Axios.spread((...allData) => {\r\n                const allDataApi = allData[0].data.meals;\r\n                const getDataPrice = allData[1].data;\r\n\r\n                const keys = Object.keys(allDataApi);\r\n                let mergedData = keys.map(key => {\r\n                    return {\r\n                        ...allDataApi[key],\r\n                        ...getDataPrice[key],\r\n                        price: getDataPrice[key].price\r\n                    };\r\n                })\r\n                setData(mergedData)\r\n                setFilterData(mergedData);\r\n            })\r\n        )\r\n    }\r\n    useEffect(() => {\r\n        fetchData();\r\n    }, [])\r\n\r\n    const handleAdd = async (id, price) => {\r\n        await axios({\r\n            method: 'post',\r\n            url: 'http://localhost:8080/favorite/addMeal',\r\n            data: {mealId: id, price: price},\r\n            headers: {\r\n                Authorization: 'Bearer ' + token,\r\n            },\r\n        }).catch((err) => console.log('Error:', err));\r\n\r\n    };\r\n\r\n    const handleAddToCart = async (id, price) => {\r\n        const object = {\r\n            mealId: `${id}`,\r\n            price: `${price}`,\r\n            quantity: 1,\r\n        };\r\n        let res = await axiosSpring\r\n            .post('/cart/add-meal', object, {\r\n                headers: {\r\n                    Authorization: 'Bearer ' + Cookies.get('token'),\r\n                },\r\n            })\r\n            .catch(() => {\r\n                toast.error();\r\n            });\r\n\r\n        if (res.status === 201 && res.data) {\r\n            toast.success('Add successful in Meals!');\r\n        }\r\n        // setCart({ id: id, price: price });\r\n    };\r\n\r\n    const handleChange = (e) => {\r\n        let value = e.target.value;\r\n        if (value === \"low\") {\r\n            setFilterData(data.filter(data => data.price <= 20))\r\n        } else if (value === \"medium\") {\r\n\r\n            setFilterData(data.filter(data => data.price <= 40))\r\n\r\n        } else if (value === \"high\") {\r\n\r\n            setFilterData(data)\r\n        }\r\n    }\r\n\r\n\r\n    return (\r\n        <>\r\n            <MealFilterByPrice handleChange={handleChange}/>\r\n\r\n            <div className=\"category-meals\">\r\n                <h2>{category}</h2>\r\n                <div className=\"meals-category\">\r\n                    {filterData.map((meal) => {\r\n                        return (\r\n                            <Meal\r\n                                key={meal.idMeal}\r\n                                handleAddToFav={handleAdd}\r\n                                handleAddToCart={handleAddToCart}\r\n                                {...meal}\r\n                                price={meal.price}\r\n                            />\r\n                        );\r\n                    })}\r\n                </div>\r\n            </div>\r\n        </>\r\n    );\r\n};\r\n\r\nexport default Meals;\r\n","C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\NotFound.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\authentication\\Payment.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\authentication\\ProtectedLogin.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\authentication\\ProtectedRoute.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\authentication\\Logout.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\MainPage.jsx",[],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\meals\\Favorites.jsx",["164"],"C:\\Users\\Artyz\\OneDrive\\Desktop\\Advance\\Glovo\\glovo-front-end\\src\\components\\search\\SearchItems.jsx",["165"],"import Re